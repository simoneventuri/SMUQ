start(main)

  start(model)
    type = tfun
    start(type)
      label = tfun
      silent = false
      start(function1)
        type = ishigami
        start(type)
          response_name = output1
          label = output1
          a = 1.0
          b = 7.0
          c = 0.1
          start(parameters)
            x1_dependency = p1
            x2_dependency = p2
            x3_dependency = p3
          end(parameters)
        end(type)
      end(function1)
    end(type)
  end(model)

  start(parameter_space)
    start(parameters)
      start(parameter1)
        name = parameter1
        label = p1
        start(distribution)
          type=uniform
          start(type)
            a = -3.14159265359
            b = 3.14159265359
          end(type)
        end(distribution)
      end(parameter1)
      start(parameter2)
        name = parameter2
        label = p2
        start(distribution)
          type=uniform
          start(type)
            a = -3.14159265359
            b = 3.14159265359
          end(type)
        end(distribution)
      end(parameter2)
      start(parameter3)
        name = parameter3
        label = p3
        start(distribution)
          type=uniform
          start(type)
            a = -3.14159265359
            b = 3.14159265359
          end(type)
        end(distribution)
      end(parameter3)
    end(parameters)
  end(parameter_space)

  start(responses)
    start(response1)
      name = response1
      label = output1

      start(coordinates)
        labels = x
        start(values)
          source = internal
          start(source)
            start(array)
              row1 = 1.0
            end(array)
          end(source)
        end(values)
      end(coordinates)

    end(response1)

  end(responses)
  
  start(analysis)
    type=surrogate_modeling

    start(type)
      type=polychaos
      start(type)
        silent = false
        basis_scheme = numerical

        start(index_scheme)
          type = hyperbolic
          start(type)
            normq = 1
            order=0
            min_order=0
            max_order=50
          end(type)
        end(index_scheme)

        start(method)
          type=sparse
          start(type)
            silent=false
            stop_error=.0001

            start(solver)
              type = lar
              start(type)
                type = subsetmcv
                start(type)
                  start(cross_validation)
                    type = loo
                    start(type)
                    end(type)
                  end(cross_validation)
                end(type)
              end(type)
            end(solver)

            start(sampler)
              start(scheme)
                type = lhs
                start(type)
                  nb_samples = 50
                  max_nb_samples = 1000
                end(type)
              end(scheme)
            end(sampler)

          end(type)
        end(method)

      end(type)
    end(type)

  end(analysis)

end(main)
